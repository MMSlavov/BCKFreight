@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        this.Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
    }
}

<div class="p-3">
    <div class="row">
        <partial name="_SettingsNav" />
    </div>
    <div class="content">
        @RenderBody()
    </div>
</div>
