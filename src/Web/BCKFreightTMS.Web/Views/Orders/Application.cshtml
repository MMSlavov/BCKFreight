@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer
@model BCKFreightTMS.Web.ViewModels.Orders.CarrierOrderApplicationModel
@{
    var orderTos = this.Model.OrderTos;
    var companyName = this.Model.OrderCreatorCompany.Name;
}
<head>
    <style>
        .head {
            margin-bottom: 0px;
            font-size: 14px;
        }

        body p {
            margin-bottom: 4px;
        }

        tr td{
            font-weight: bold;
        }

        .application {
            font-size: 19px;
        }
    </style>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
</head>
<div class="application">
    <div class="d-flex justify-content-between">
        <img src="data:image/png;base64,"
             style="text-align: left"
             height="107"
             width="290" />
        <div style="text-align: right">
            <p class="head"><b>ВЪЗЛОЖИТЕЛ</b></p>
            <p class="head">@Model.OrderCreatorCompany.Name</p>
            @*<p>@Model.OrderToCompanyAddressAddressCity</p>*@

        <p class="head">@Model.OrderCreatorCompany.AddressAddressStreetLine, @Model.OrderCreatorCompany.AddressAddressCity, @Model.OrderCreatorCompany.TaxCountryName</p>@*, City name, Country name*@

            <p class="head">ИД. Код: @(Model.OrderCreatorCompany.TaxNumber)</p>

            @*<p class="head">ИН по ЗДДС: @($"BG{Model.CreatorCompany.TaxNumber}")</p>*@

            <p class="head">МОЛ: @Model.OrderCreatorCompany.AddressMOLFirstName @Model.OrderCreatorCompany.AddressMOLLastName</p>

            <p class="head">Тел: @Model.OrderCreatorCompany.ComunicatorsMobile1</p>
        </div>
    </div>

    <div style="text-align: center">
        <p style="font-size:20px"><strong>ЗАЯВКА</strong></p>

        <p>за международен/вътреобщностен автомобилен транспорт</p>

        <p>№ <span class="refNum">@Model.ReferenceNum</span> / @DateTime.Now.ToString("dd.MM.yyyyг.")</p>
    </div>

    <p>Изпълнител: </p>
    <div style="padding-left: 3rem;">
        <span>@Model.Company.Name</span>
        @*<p>@Model.OrderToCompanyAddressAddressCity</p>*@

        <p>@Model.Company.AddressAddressStreetLine</p>

        <p>ИН по ЗДДС: @(Model.Company.TaxNumber.Length <= 10?"BG":"")@(Model.Company.TaxNumber)</p>

        <p>МОЛ: @Model.Company.AddressMOLFirstName @Model.Company.AddressMOLLastName</p>

        <p>Тел: @Model.Company.ComunicatorsMobile1</p>
    </div>

    @foreach (var orderTo in orderTos)
    {
        var documentation = orderTo.Documentation;
        <p><b>· @orderTo.VehicleRegNumber @(orderTo.VehicleTrailerRegNumber is null ? "": $"/ {orderTo.VehicleTrailerRegNumber}")</b></p>
        @if (!string.IsNullOrWhiteSpace(orderTo.CargoVehicleRequirements))
        {
            <p><b><u>Model.Cargo.VehicleRequirements</u></b></p>
        }

        <table class="table table-bordered">
            @*<thead style="margin-bottom: 0px;">
                <tr>
                    <th>
                        Id
                    </th>
                    <th>
                        Name
                    </th>
                    <th>
                        Type
                    </th>
                    <th>
                        Contacts
                    </th>
                </tr>
            </thead>*@
            @foreach (var action in orderTo.OrderActions)
            {
                <tr>
                    <td>@localizer[action.TypeName]</td>
                    <td>
                        @(action.Address.City is null?"":$"{action.Address.City}, ")
                        @(action.Address.Area is null || action.Address.City == action.Address.Area? "":$"{action.Address.Area}, ")
                        @(action.Address.Postcode is null?"":$"{action.Address.Postcode}, ")
                        @action.Address.StreetLine
                    </td>
                    <td>@action.Until.ToLocalTime().ToShortDateString()</td>
                    <td>@action.Details</td>
                </tr>
            }
        </table>

        <p>Данни за товара:</p>

        <p>· Стока: <b>@orderTo.Cargo.Name@(orderTo.Cargo.Quantity <= 1?"":$" - {this.localizer[orderTo.Cargo.TypeName].Value}") - @orderTo.Cargo.WeightGross т.@Html.Raw(orderTo.Cargo.Details is null?"":$"; {Html.Raw(orderTo.Cargo.Details)}")</b></p>

        <p>· Придружаващи документи: </p>
        <div class="pl-5">
            @if (@documentation.CMR)
            {<div><i class="fas fa-check"></i><b>&nbsp;CMR</b></div>}
            @if (@documentation.BillOfLading)
            {<div><i class="fas fa-check"></i><b>&nbsp;@localizer["Bill of lading"]</b></div>}
            @if (@documentation.AOA)
            {<div><i class="fas fa-check"></i><b>&nbsp;@localizer["Act of acceptance"]</b></div>}
            @if (@documentation.DeliveryNote)
            {<div><i class="fas fa-check"></i><b>&nbsp;Delivery note</b></div>}
            @if (@documentation.PackingList)
            {<div><i class="fas fa-check"></i><b>&nbsp;Packing list</b></div>}
            @if (@documentation.ListItems)
            {<div><i class="fas fa-check"></i><b>&nbsp;List items</b></div>}
            @if (@documentation.Invoice)
            {<div><i class="fas fa-check"></i><b>&nbsp;Invoice</b></div>}
            @if (@documentation.BillOfGoods)
            {<div><i class="fas fa-check"></i><b>&nbsp;@localizer["Bill of goods"]</b></div>}
        </div>

        <p>Цена и начин на плащане: <b>@orderTo.PriceNetOut</b> @orderTo.CurrencyName @(this.Model.NoVAT ? "" : "+ ДДС")</p>
        <hr style="border: none; height: 2px; background-color: black;"/>
    }
    <p>· Платец на навлото: @companyName</p>

    <p>· Начин и срок на плащане: В @this.Model.OrderDueDaysTo дневен срок, след получаване на Ваша фактура и оригинал на заверени – подписани и/или подпечатани 3бр. CMR-товарителници, без забележки, за действително превозваният товар, на <b>адреса за кореспонденция: <b style=" text-decoration: underline;">ул. „Хаджи Димитър“№10, гр. Търговище.</b></b></p>

    <p>· <strong>ФАКТУРИ С НЕПРАВИЛНИ ДАННИ, НЕПЪЛНИ КОМПЛЕКТИ С ДОКУМЕНТИ, НЕ СЕ ОБРАБОТВАТ И ЩЕ БЪДАТ ВРЪЩАНИ ОБРАТНО С КУРИЕР ЗА СМЕТКА НА ПРЕВОЗВАЧА. ДАТАТА НА ПАДЕЖ ЗАПОЧВА ДА ТЕЧЕ ОТ ДАТАТА НА ПОЛУЧАВАНЕ НА ПЪЛНИЯ КОМПЛЕКТ ДОКУМЕНТИ. </strong></p>

    <p><strong>5. Задължения на Превозвача:</strong></p>

    <p>При изпълнение на задълженията си, превозвачът е длъжен да действа с грижата на добър стопанин/търговец.</p>

    <p>Превозвачът гарантира, че използваните транспортни средства и наетия от него персонал, отговаря на всички нормативни изисквания, в това число Превозвачът разполага с всички разрешителни, лицензи и необходими документи за извършване на заявения превоз. Превозното средство трябва да се намира в изправно техническо състояние след редовен технически преглед.</p>

    <p>При транспорт на опасни товари превозвачът гарантира, че водачите на превозните средства са обучени според условията на ADR Конвенцията, а превозните средства отговарят на изискванията на ADR Конвенцията.</p>

    <p><strong>Превозвачът е длъжен да представи на @companyName валидна застраховка „Отговорност на превозвача“ за заявения с настоящата заявка превоз, като спазва стриктно задълженията си по застрахователния договор. Превозвачът е длъжен да паркира автомобила само на охраняеми паркинги и да не оставя превозното средство без надзор.</strong></p>

    <p>Превозвачът гарантира, че неговите служители/подизпълнители ще изпълняват задълженията си според изискванията на закона и настоящата заявка, както и се въздържат от действия, които могат да навредят на доброто име на @companyName, на изпращача и/или получателя на товара, ще бъдат в изряден външен вид, и ще спазват общоприетите правила за добро поведение. </p>

    <p><strong>По време на приемането на товара превозвачът е длъжен: </strong></p>

    <p>· Да следи правилното нареждане и укрепване на товара, което да предотврати разместването му;</p>

    <p>· Да провери точността на данните в товарителницата относно брой и маркировка на колетите и впише в случай на несъответствие забележка в товарителницата;</p>

    <p>· Да провери видимото състояние на превозваната стока и нейната опаковка и впише забележка, в случай на нарушена опаковка или недостатъчна такава; </p>

    <p>· Да нанесе забележките си в товарителницата, при невъзможност да брои в случай че няма достъп до товарното помещение или не се спазват неговите указания по време на товарене;</p>

    <p>· Да подпише и подпечата всички екземпляри на товарителницата;</p>

    <p>По време на транспорта, Превозвачът е длъжен:</p>

    <p>Да спазва изискването автомобилът да бъде снабден с колани, минимум по един на всеки линеен метър. <strong>В случай на неспазване, превозвачът дължи неустойка 100,00лв.</strong></p>

    <p>Да дава ежедневна информация за движението на автомобила. <strong>Превозвачът дължи неустойка от 100,00лв. за всеки ден неизпълнение.</strong></p>

    <p>Да спазва последващите нареждания и указания от страна на @companyName.</p>

    <p>В случай, че представител на митническата администрация разпломбира превозното средство и изземе част от товара, Превозвачът е длъжен да следи за това, иззетото количество да бъде вписано в товарителницата или да бъде съставен протокол, като тези действия трябва да бъдат удостоверени с подпис и митнически печат.</p>

    <p>Превозвачът се задължава да не се договаря и извършва пряко за изпращача или получателя, който е клиент на @companyName, превоз на товари, освен ако Превозвачът вече има договорни взаимоотношения с този клиент. <strong>При неизпълнение, Превозвачът дължи на @companyName неустойка в размер на навлото, за последния извършен по заявка на @companyName превоз, но не по-малко от 1000,00лв.С настоящето Превозвачът декларира, че такива взаимоотношения не съществуват. </strong></p>

    <p>Превозвачът е длъжен своевременно да информира @companyName в писмен вид, при промяна на обстоятелства, свързани или относно търговската му регистрация, лиценз за превоз на товари, както и подновяване на ЧМР застраховката. </p>
    <p>Превозвачът няма право да възложи на подизпълнител извършването на заявения превоз, освен с писмено съгласие на @companyName. Не се разрешава публикуването на товари в интернет пространството и транспортно-спедиторските борси. </p>

    <p>При катастрофа, пожар, кражби или друг инцидент, Превозвачът е длъжен незабавно да информира @companyName, както и компетентните полицейски и други власти и изисква съставянето на официални документи, удостоверяващи събитието. Превозвачът е длъжен да съдейства на @companyName за изясняването на всякакви случаи на неизпълнение на закона и настоящата заявка. </p>

    <p>Превозвачът е длъжен да не разпространява и да не предоставя на трети лица никаква търговска или административна информация за @companyName, станала известна във връзка или по повод на настоящата заявка. </p>

    <p><strong>6. Отговорност и санкции:</strong></p>

    <p><strong>6.1</strong> Всички транспорти са подчинени на и се извършват в съответствие с Конвенцията относно договора за международен превоз на стоки по шосе (CMR), актуално издание.</p>

    <p><strong>6.2</strong> Превозвачът е отговорен за цялостната или частична липса и/или повреда на товара от момента на приемането му до момента на доставянето му. Последният е длъжен в рамките на 20 дни от получаване на рекламация за цялостната или частична липса и/или повреда на товара да предяви претенция за обезщетение за възникналата щета към своя застраховател и да уведоми @companyName за своето становище, респективно за становището на своя застраховател, по тази рекламация. В случай че след горепосочения срок такова становище не е предоставено на @companyName, ще се смята, че Превозвачът не оспорва отправената към него рекламация по основание и размер, и се задължава да я изплати в 15-дневен срок. </p>

    <p><strong>6.3 При престой на камион над 48 часа за обмитяване/товарене/разтоварване по вина на @companyName, последният дължи неустойка в размер на 100,00лв. за започнати 24 часа, ако в 5 дневен срок от деня на натоварване Превозвачът предяви искане.</strong></p>

    <p><strong>6.4 Ако в деня на товарене @companyName оттегли заявката за превоз, последният дължи неустойка в размер на 50,00лв., ако в 5-дневен срок Превозвачът предяви искане. </strong></p>

    <p><strong>6.5</strong>Превозвачът гарантира спазването на всички трудово-правни и социално-правни разпоредби свързани с неговата дейност, в това число и законови разпоредби на други държави, на чиято територия заявката за превоз се изпълнява, когато законите на тази чужда страна могат пряко или косвено да бъдат приложими във връзка с изпълнението на заявката за превоз. Тук се има в предвид, но не само разпоредби относно минималните заплати и т.н.</p>

    <p><strong>6.5A</strong>Превозвачът е задължен да осигури спазване на задълженията по т.6.5. от неговите подизпълнители и временни служители/работници.</p>

    <p><strong>6.5B</strong>При поискване, Превозвачът е длъжен да предостави доказателство за спазване на задълженията му по т.6.5.</p>

    <p><strong>6.5CЗа всяко нарушение на задълженията му по т. 6.5., Превозвачът дължи неустойка в размер на 5000,00€, която ще бъде дължима за плащане веднага. </strong></p>

    <p><strong>6.5D</strong> Превозвачът е задължен да обезщети @companyName по всички искове от трети страни, по-специално, но не само от служители на превозвача, претенции на подизпълнители, на работниците и служителите на подизпълнител, както и временни служители, които претенции са възникнали по повод на или във връзка с разпоредбите посочени в т6.5., като резултат на изпълнението на заявката за превоз. Задължението за обезщетение се отнася и за искове срещу Превозвача от страна на данъчно-осигурителни и други финансови органи и институции. </p>

    <p><strong>6.5E</strong> @companyName ще уведоми Превозвачът своевременно, ако претенции свързани с прилагането на разпоредбите посочени в т.6.5. по горе са предявени срещу @companyName от служители на Превозвача, подизпълнители или временно работещи за Превозвача, както и изпълняващи дейности за Превозвача на база граждански договори, или ако на @companyName стане известно, подобни претенции са предявени към трети страни от служители на Превозвача, подизпълнители или временно работещи за Превозвача, или изпълняващи дейности за Превозвача на база за граждански договори, данъчно-осигурителни или други финансови органи и институции. </p>

    <p><strong>6.5F</strong> Ако по повод или във връзка със споменатите в т6.5 разпоредби или във връзка с изпълнение на заявка за превоз от @companyName, ръководител или служител/работник на @companyName бъде осъден с влязло в сила решение за неспазване на разпоредбите по т. 6.5. по небрежност от страна на Превозвача, последният е длъжен да обезщети @companyName за наложено наказание, освен ако това не представя възпрепятстване на правосъдието. Превозвачът е длъжен да плати обезщетение на @companyName или съответната страна за разходи направени по делото, в това число държавни такси, адвокатски-консултантски хонорар. </p>

    <p><strong>6.5G</strong> Превозвачът е длъжен да уведоми @companyName незабавно за започнало административно или наказателно производство във връзка с прилагането на посочените в т.6.5. разпоредби, или ако на Превозвача е станало известно, че административно или наказателно производство на база прилагането на посочените в т.6.5 разпоредби е свързано с подизпълнители на Превозвача, временно работещи за Превозвача или изпълняващи дейности за Превозвача на база на граждански договори. </p>

    <p><strong>6.6</strong> При изпълнение на задълженията си по настоящата заявка, Превозвачът е длъжен да спазва всички приложими нормативни актове, ръководни принципи и други правила. В това число:</p>

    <p>· Превозвачът е длъжен да спазва нормите за борба с подкупите и корупцията (също така съответното законодателство на Обединеното кралство, когато е приложимо)</p>

    <p>· Превозвачът е длъжен да съблюдава правилата по организацията на времето за управление, прекъсвания, дневните и седмичните почивки на водачите във връзка с регламент (ЕО) 561 от 2006г. на Европейския парламент и Съвета на Европейския съюз, и във връзка с забраните за движение на тежкотоварни автомобили;</p>

    <p>· Превозвачът гарантира, че не е замесен с лица, групи, образуванията с които е забранено извършването на търговска дейност, в съответствие с Резолюции на ООН 1376/2001г. и 1390/2002г. разпоредби на ЕО 2580/2001г. и 881/2002г.</p>

    <p><strong>6.7</strong> @companyName не носи отговорност за искове, загуби и/или щети, възникнали във връзка с неизпълнение от страна на Превозвача на задълженията му по настоящата заявка и закона.</p>

