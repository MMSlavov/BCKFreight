@using BCKFreightTMS.Common.Enums;
@using BCKFreightTMS.Data.Models
@using Microsoft.AspNetCore.Identity
@inject UserManager<ApplicationUser> userManager
@inject BCKFreightTMS.Services.Data.IFinanceService financeService
@model System.Collections.Generic.Dictionary<string, string>
@{
    this.Layout = null;
    var user = await this.userManager.GetUserAsync(this.User);
    var companyCurrency = user.Company.TaxCurrency.Name;
}


<table class="table table-striped table-hover rounded mb-1 border-0">
    <tbody>
        @foreach (var rox in this.Model.Skip(1))
        {
            <tr>
                <td class="text-center">
                    <span class="text-bold">@rox.Key</span>
                </td>
                <td>
                    <span>@rox.Value</span>
                </td>
            </tr>
        }
    </tbody>
</table>

